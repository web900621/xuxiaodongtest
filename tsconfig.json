{
   "compilerOptions": {
    "jsx": "react",
    "lib": ["es6", "dom"],
    "rootDir": "app",
    "module": "commonjs",
    "target": "es5",
    "baseUrl": "./",
    "sourceMap": true,
    "moduleResolution": "node",
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "strictNullChecks": true
    },
    "exclude": [
      "node_modules",
      "app/uicomponents"
    ]
    
}